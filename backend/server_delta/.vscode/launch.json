    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach (Remote Debug)",
            "type": "pythonExperimental",
            "request": "attach",
            "port": 8001,
            "host": "localhost"
        },
       {
           "name": "Django",
           "type": "pythonExperimental",
           "request": "launch",
           "program": "${workspaceFolder}/manage.py",
           "args": [
               "runserver",
               "--noreload",
               "--nothreading"
           ],
           "django": true
       },
            {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": [],
            "showLog": true
        }
    ]
}